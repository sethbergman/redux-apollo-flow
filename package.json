{
  "name": "redux-apollo-flow",
  "description": "How to scale a modern React / Redux app with Apollo and GraphQL",
  "version": "1.1.1",
  "author": "Seth Bergman <hello@sethbergman.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "apollo",
    "graphql",
    "es6",
    "babel",
    "webpack",
    "sentry",
    "architecture",
    "webpack-dev-middleware",
    "webpack-hot-middleware"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sethbergman/redux-apollo-flow.git"
  },
  "engines": {
    "node": "8.1.x",
    "npm": "5.4.1"
  },
  "bugs": "https://github.com/sethbergman/redux-apollo-flow/issues",
  "scripts": {
    "clean:build": "rimraf build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "npm run clean:build",
    "start": "webpack-dashboard -c cyan -- node dev-server.js",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": "npm run build:html",
    "build:serve": "http-server build/client -p 3003 --cors -o",
    "lint": "eslint config src/js/** --ext .js",
    "jscs": "jscs src/js/",
    "typecheck": "flow",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch",
    "tunnel": "lt --port 3000"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "chai-jquery": "2.0.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-prettier": "2.2.0",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "0.54.1",
    "http-server": "0.10.0",
    "jquery": "3.2.1",
    "jscs": "3.0.7",
    "jsdom": "11.2.0",
    "mocha": "3.5.3",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "prettier": "1.7.0",
    "prettier-webpack-plugin": "0.2.2",
    "react-addons-test-utils": "15.6.0",
    "redbox-react": "1.5.0",
    "redux-devtools": "3.4.0",
    "redux-devtools-dock-monitor": "1.1.2",
    "redux-devtools-log-monitor": "1.3.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-slider-monitor": "1.0.7",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.6",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "autoprefixer": "7.1.4",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.5",
    "express": "4.15.4",
    "lodash": "4.17.4",
    "raven-js": "3.17.0",
    "react": "16.1.0",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.2",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "reselect": "3.0.1"
  }
}
